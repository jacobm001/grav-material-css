{% extends 'partials/base.html.twig' %}

{% set collection = page.collection() %}

{% block content %}

<div class="row">
	<div class="col s12 m10 offset-m1 xl8 offset-xl2">
		<div class="card">
			<div class="card-content">
				<span class="card-title">{{ page.title }}</span>
				{{ page.content }}
			</div>
			{# <div class="card-action">
				<a href="{{ page.url }}">Contact Me</a>
			</div> #}
		</div>
	</div>
</div>

<div class="row">
{% for child in collection %}
	{% include 'partials/gallery_preview.html.twig' with {'photography':page, 'page':child, 'truncate':true } %}
{% endfor %}
</div>

{% endblock %}