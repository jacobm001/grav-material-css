{% embed 'partials/base.html.twig' %}

	{% set collection = page.collection() %}


	{% block content %}

	<div class="container">
		
			<div class="row">
				<div class="col s12 m8 l8 ">
					<div class="row">
						{% for child in collection %}
							{% include 'partials/blog_item.html.twig' with { 'blog':page, 'page':child, 'truncate':true } %}
						{% endfor %}
					</div>
				</div>

				<div class="col m4 l4">
					<div class="row s12">
						{% include 'partials/blog_sidebar.html.twig' %}
					</div>
				</div>
			</div>

		<!-- </div> -->
	</div>

	{# {% do assets.addJs('theme://js/stackexchange.js', {group: 'content'}) %} #}
	{% endblock %}

{% endembed %}